<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape>
            <!-- 实心 -->
            <solid android:color="#FFFFFF" />

            <stroke
                android:width="0.5dp"
                android:color="#0099FF" />

            <!-- 圆角 -->
            <corners android:radius="20dp" />
        </shape>
    </item>
</selector>